{
  "name": "tapconvey",
  "version": "0.1.0",
  "description": "Reads TAP-formatted data from STDIN to display test results in a web application like goconvey",
  "main": "src/index.js",
  "typings": "src/index.js",
  "scripts": {
    "clean:ts": "find src www -type f -name '*.ts' ! -name '*.d.ts' | sed 's/\\.ts$//' | xargs -I file find file.map file.js file.d.ts -delete",
    "dev:www": "tap test/**/*.js | ts-node www/_dev_server.ts",
    "build:www": "webpack --config www/webpack.config.js -v --display-error-details",
    "test:src": "mocha --compilers ts:ts-node/register --recursive 'src/**/*.test.ts?(x)'",
    "test:www": "mocha --require ignore-styles --require ./www/_require_jsdom.js --compilers ts:ts-node/register --recursive 'www/**/*.test.ts?(x)'",
    "test:all": "yarn run test:src && yarn run test:www",
    "test": "yarn run cover && nyc check-coverage --statements 90 --lines 90",
    "cover": "nyc yarn run test:all",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "watch": "nodemon -e ts,tsx --exec \"yarn run $1\""
  },
  "keywords": [
    "tap",
    "goconvey",
    "runner",
    "test",
    "tape"
  ],
  "author": "gordon@gordonburgett.net",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-enzyme": "^0.5.5",
    "@types/enzyme": "^2.5.39",
    "@types/es6-shim": "^0.31.32",
    "@types/express": "^4.0.34",
    "@types/js-yaml": "^3.5.29",
    "@types/mocha": "^2.2.33",
    "@types/node": "0.0.2",
    "@types/react": "^0.14.54",
    "@types/react-dom": "^0.14.19",
    "@types/sinon": "^1.16.34",
    "@types/superagent": "^2.0.36",
    "@types/webpack": "^2.0.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "js-yaml": "^3.7.0",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "mobx": "^2.7.0",
    "mobx-react": "^4.0.4",
    "mobx-react-devtools": "^4.2.10",
    "mocha": "^3.2.0",
    "mocha-jsdom": "^1.1.0",
    "mocha-tap-reporter": "^0.1.3",
    "node-sass": "^4.1.1",
    "nyc": "^10.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "next",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "superagent": "^3.3.1",
    "tap": "^8.0.1",
    "tape": "^4.6.3",
    "ts-loader": "^1.3.1",
    "ts-node": "^1.7.0",
    "typescript": "^2.0.10",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "debug": "^2.6.0",
    "express": "^4.14.0",
    "nodemon": "^1.11.0",
    "tap-parser": "^3.0.4"
  }
}