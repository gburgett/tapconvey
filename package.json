{
  "name": "tapconvey",
  "version": "0.1.0",
  "description": "Reads TAP-formatted data from STDIN to display test results in a web application like goconvey",
  "main": "src/index.js",
  "typings": "src/index.js",
  "scripts": {
    "dev-www": "webpack-dev-server --inline --config www/webpack.config.js",
    "build-www": "webpack --config www/webpack.config.js -v --display-error-details",
    "test": "mocha --compilers ts:ts-node/register --recursive 'src/**/*.test.ts'",
    "test-watch": "mocha -w --compilers ts:ts-node/register --recursive 'src/**/*.test.ts'"
  },
  "keywords": [
    "tap",
    "goconvey",
    "runner",
    "test",
    "tape"
  ],
  "author": "gordon@gordonburgett.net",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.33",
    "@types/node": "0.0.2",
    "@types/react": "^0.14.54",
    "@types/react-dom": "^0.14.19",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "mocha-tap-reporter": "^0.1.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "source-map-loader": "^0.1.5",
    "tap": "^8.0.1",
    "tape": "^4.6.3",
    "ts-loader": "^1.3.1",
    "ts-node": "^1.7.0",
    "typescript": "^2.0.10",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "nodemon": "^1.11.0",
    "tap-parser": "^3.0.4"
  }
}
